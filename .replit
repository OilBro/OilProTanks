modules = ["nodejs-20", "web", "postgresql-16"]
# Force the app to bind on 5000 to match port mappings below
run = "FORCE_PORT=5000 npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["jq", "graphicsmagick", "imagemagick"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
# Ensure production server also binds to 5000 (mapped to external 80 below)
run = ["bash", "-lc", "FORCE_PORT=5000 npm run start"]

[[ports]]
localPort = 4500
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 32779
externalPort = 3000

[[ports]]
localPort = 33735
externalPort = 3002

[[ports]]
localPort = 34889
externalPort = 3003

[[ports]]
localPort = 37899
externalPort = 4200

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "FORCE_PORT=5000 npm run dev"
waitForPort = 5000

[agent]
